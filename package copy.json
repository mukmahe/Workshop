
function log(message) {
  cy.task("log", message)
  let today = new Date();
  let time = "Time- "+today.getHours() + ":" + today.getMinutes() + ":" + today.getSeconds()
  addTestContext(message,time)
}

function addTestContext(title, value) {
  cy.once('test:after:run', test => addContext({ test }, { title, value }))
}
